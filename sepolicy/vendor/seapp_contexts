# Use a custom app domain for oneplus camera
user=_app isPrivApp=true seinfo=oneplus name=com.oneplus.camera domain=oneplus_camera_app type=app_data_file levelFrom=all

user=_app isPrivApp=true seinfo=oneplus name=org.halogenos.settings.device.extra domain=oneplus_extra_settings_app type=app_data_file levelFrom=all
